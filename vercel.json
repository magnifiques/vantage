{
  "buildCommand": "expo export:web",
  "outputDirectory": "web-build",
  "devCommand": "expo start",
  "cleanUrls": true,
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "web-build/**",
      "use": "@vercel/static"
    }
  ]
}
